  </param>
  <param>
    <name>Interpolation Tap Length</name>
    <key>interp_len</key>
    <value>1</value>
    <type>int</type>
    <hide>
#if $mtype() in ('signal_exciter.OFDM','signal_exciter.DSB', 'signal_exciter.DSBSC', 'signal_exciter.FM', 'signal_exciter.USB', 'signal_exciter.LSB','signal_exciter.CWMORSE')
  none
#else
  all
#end if
    </hide>
  </param>
  <param>
    <name>Taper</name>
    <key>taper</key>
    <value>[]</value>
    <type>float_vector</type>
    <hide>
#if $mtype() in ('signal_exciter.OFDM')
  none
#else
  all
#end if
    </hide>
  </param>
  <param>
    <name>Sample Base Overlap</name>
    <key>samp_overlap</key>
    <value>0</value>
    <type>int</type>
    <hide>
#if $mtype() in ('signal_exciter.OFDM')
  none
#else
  all
#end if
    </hide>
  </param>
  <param>
    <name>Enable Fractional?</name>
    <key>efso</key>
    <value>False</value>
    <type>enum</type>
    <hide>part</hide>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
  </param>
  <param>
    <name>Fractional Symbol Offset</name>
    <key>fso</key>
    <value>0.</value>
    <type>real</type>
    <hide>
#if $efso() in ('True')
  part
#else
  all
#end if
    </hide>
  </param>
  <param>
    <name>Seed</name>
    <key>seed</key>
    <value>-1</value>
    <type>int</type>
    <hide>part</hide>
  </param>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
