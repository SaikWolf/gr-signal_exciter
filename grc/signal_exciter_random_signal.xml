#if $mtype() in ('signal_exciter.CWMORSE',)
  none
#else
  all
#end if
    </hide>
  </param>
  <param>
    <name>Words Per Minute</name>
    <key>wpm</key>
    <value>20</value>
    <type>int</type>
    <hide>
#if $mtype() in ('signal_exciter.CWMORSE',)
  none
#else
  all
#end if
    </hide>
  </param>
  <param>
    <name>Base Word</name>
    <key>base_word</key>
    <value>False</value>
    <type>enum</type>
    <hide>
#if $mtype() in ('signal_exciter.CWMORSE',)
  none
#else
  all
#end if
    </hide>
    <option>
      <name>PARIS</name>
      <key>False</key>
    </option>
    <option>
      <name>CODEX</name>
      <key>True</key>
    </option>
  </param>
  <param>
    <name>Overlapping Symbols</name>
    <key>overlap</key>
    <value>4</value>
    <type>int</type>
